<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tian.second_project.dao.OrderDao">


    <insert id="createOrder" parameterType="Order" useGeneratedKeys="true" keyProperty="id">
        insert into stock_order(id,sid,name,create_time) value (id=#{id},name =#{name},sid=#{sid},create_time=#{create_time})
    </insert>


</mapper>